{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"exports": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"settings": {
		"jsdoc": {
			"preferredTypes": {
				"VectorHeaderObserver": "VectorHeaderObserver"
			}
		}
	},
	"rules": {
		"compat/compat": "off",
		"mediawiki/class-doc": "off"
	}
}
